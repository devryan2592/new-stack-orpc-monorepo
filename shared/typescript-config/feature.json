{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Features",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "declaration": true,
    "composite": true,
    "baseUrl": "."
  }
}
